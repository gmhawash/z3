= form_for @gate do |f|
  -if @gate.errors.any?
    #error_explanation
      %h2= "#{pluralize(@gate.errors.count, "error")} prohibited this gate from being saved:"
      %ul
        - @gate.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :image
    = f.text_field :image
  .field
    = f.label :symbol
    = f.text_field :symbol
  .actions
    = f.submit 'Save'
