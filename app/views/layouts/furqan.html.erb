<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en" class="js">
<head>
  <meta name="generator" content= "HTML Tidy for Linux/x86 (vers 11 February 2007), see www.w3.org" />
  <meta http-equiv="Content-Type" content="text/html; charset=us-ascii" />

  <title><%= page_title %></title>
  <%= stylesheet_link_tag :all %><%= stylesheet_link_tag 'layout' %><%= javascript_include_tag :defaults %><%= csrf_meta_tag %>
  <link rel="shortcut icon" href= "http://www.ridepdw.com/sites/all/themes/pdw/favicon.ico" type="image/x-icon" />
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js" type= "text/javascript"> </script>
  <script src="javascripts/nivo-slider/jquery.nivo.slider.js" type="text/javascript"> </script>
</head>

<body class= "not-logged-in front full-node node-type-page layout-main postscript-first-middle section-home browser-ie">
<div id="wrapper" class="clearfix">
    <div id="header" class="clearfix" style="border-top-left-radius: 20px 20px; border-top-right-radius: 20px 20px;">
      <div id="header-inner">
        <div id="basmalah">&#054;</div>
        <div id="logo">
        </div>

      </div><!--/ header-inner -->
    </div><!-- /header -->

    <%= render :partial => 'shared/menu'  %>
    <div id="banner" class="clearfix">
      <div id="name">
        <%= image_tag logo %>
      </div>
      <div id="photo">
        <%= render :partial => 'shared/slider' %>
      </div>
    </div>

      <div id="main" class="clearfix" >
            <div id="content-wrapper" class="clearfix">
              <div id="content-inner" class="clearfix">
                <div id="content">
                <%= yield :sidebar %>
                <%= yield %>
                  </div><!-- /content -->
                </div><!-- /content-inner -->
              </div><!-- /content-wrapper -->
        </div><!-- /main -->

      <%= yield :featured %>
        <div id="footer" class="clearfix">
          <div id="footer-wrapper" class="clearfix">
            <div id="footer-first">
            </div><!-- /footer-first -->

            <div id="footer-last">
            </div><!-- /footer-last -->
          </div><!-- /footer-wrapper -->
        </div><!-- /footer -->
      </div><!-- /wrapper -->
    </div><!-- /page -->

  </div>
</body>
</html>

